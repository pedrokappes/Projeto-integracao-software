// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Carro {
        id Int @id @default(autoincrement())
        placa String
        modelo String
        clienteId Int
}

model Vaga {
    id Int @id @default(autoincrement())
    vazia Boolean
    carroId Int
    horaEntrada Float
    horaSaida Float
}